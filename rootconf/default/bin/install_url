# Deprecated legacy command

prog=$(basename $0)
echo "NOTICE: The '$prog' command is deprecated and will be removed in 2019.02."

new_prog=onie-nos-install
echo "NOTICE: Use '$new_prog' instead."

$new_prog "$@"
